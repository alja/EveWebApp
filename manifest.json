{
    "sap.app": {
        "id": "currentdir"
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "currentdir.view.MainMain",
            "type": "XML",
            "async": true,
            "id": "Eve7"
        },
        "resources": {
            "css": [
                {
                  "uri": "css/eve.css"
                }
              ]
        },
        "dependencies": {
            "libs": {
                "sap.ui.core": {},
                "sap.ui.commons": {},
                "sap.ui.layout": {},
                "sap.m": {},
                "sap.uxap": {},
                "sap.ui.table": {}
            }
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "currentdir.view",
                "controlAggregation": "pages",
                "controlId": "myApp"
            },
            "routes": [{
                "name": "Default",
                "pattern": "",
                "target": ["Main"]
            }, {
                "name": "View",
                "pattern": "view/:viewName:",
                "target": ["GL"]
            }],
            "targets": {
                "Main": {
                    "transition": "slide",
                    "clearAggregation": false,
                    "viewName": "Main"
                },
                "GL": {
                    "transition": "slide",
                    "clearAggregation": false,
                    "viewName": "GL"
                }
            }
        }
    }
}
